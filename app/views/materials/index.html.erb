<h1>Listing materials</h1>

<table>
  <thead>
    <tr>
      <th>Company name</th>
      <th>E-mail</th>
      <th>Phone number</th>
      <th>Contact person</th>
      <th> Material </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @suppliers.each do |supplier| %>
      <tr>
        <td><%= supplier.company_name %></td>
        <td><%= supplier.email %></td>
        <td><%= supplier.phone_number %></td>
        <td><%= supplier.contact_person %></td>
        <td><%= supplier.materials %> </td>
        <td><%= link_to 'Show', [material.supplier, material] %></td>
        <td><%= link_to 'Edit', edit_supplier_material_path(material.supplier, material) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Supplier', new_supplier_path %>
