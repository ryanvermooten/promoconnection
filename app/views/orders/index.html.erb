<h1>Listing orders</h1>


  </div>
<table>
  <thead>
    <tr>
      <th>  </th>
      <th> Template used </th>
      <th> Special Notes </th>
      <th> Date due </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |t| %>
      <tr>
        <td><%= t.customer %></td>
        <td><%= t.templates.pluck(:final_product).join(", ")  %></td>
        <td><%= t.special_notes %></td>
        <td><%= t.due_at %></td>
    </div>
       </tr>
    <% end %>
  </tbody>
</table>

