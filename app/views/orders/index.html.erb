<h1>Listing orders</h1>

<table>
  <thead>
    <tr>
      <th>  </th>
      <th> Template used </th>
      <th> Special Notes </th>
      <th> Date due </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |t| %>
      <tr>
        <td><%= t.customer_id %></td>
         <td><%= t.templates %></td>
        <td><%= t.special_notes %></td>
         <td><%= t.due_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Order', new_order_path %>
